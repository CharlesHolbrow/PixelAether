{
  "name": "pixel-aether-game-server",
  "version": "1.0.0",
  "description": "A Digital World in the Cloud",
  "main": "index.js",
  "devDependencies": {
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^5.1.1"
  },
  "dependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-packages": "meteor test-packages ./ --driver-package practicalmeteor:mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CharlesHolbrow/PixelAetherGameServer.git"
  },
  "author": "Charles Holbrow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CharlesHolbrow/PixelAetherGameServer/issues"
  },
  "homepage": "https://github.com/CharlesHolbrow/PixelAetherGameServer#readme",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "eslintConfig": {
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "rules": {
      "meteor/eventmap-params": [
        2, { "templateInstanceParamName": "instance" }
      ],
      "import/no-unresolved": [
        2, { "ignore": ["^meteor/"] }
      ],
      "no-multi-spaces": "off",
      "no-undef": "off",
      "curly": "off",
      "no-console": "off",
      "padded-blocks": "off"
    }
  }

}
